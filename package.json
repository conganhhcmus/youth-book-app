    "keywords": [
        "youthbooks",
        "comics",
        "books"
    ],
    "author": "Cong Anh (conganhhcmus@gmail.com)",
    "license": "MIT",
    "engines": {
        "node": "^20.0",
        "yarn": "^1.22"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.1",
        "axios": "^1.6.7",
        "classnames": "^2.5.1",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^2.0.4",
        "react-query": "^3.39.3",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.2",
        "swiper": "^11.0.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.1",
        "@commitlint/config-conventional": "^18.6.2",
        "@types/lodash": "^4.14.202",
